{
    "name": "mibo/vat",
    "type": "library",
    "description": "PHP Library for generic and/or custom properties",
    "keywords": [
        "PHP",
        "EU VAT",
        "EU",
        "European Union",
        "VAT rates",
        "VAT rate",
        "VAT"
    ],
    "authors": [
        {
            "name": "Michal Boris",
            "email": "michal.boris27@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "MiBo\\VAT\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MiBo\\VAT\\Tests\\": [
                "tests/Core/",
                "tests/"
            ]
        }
    },
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpunit/php-invoker": "^4.0",
        "phpstan/phpstan": "^1.5",
        "squizlabs/php_codesniffer": "^3.6",
        "phpstan/phpstan-strict-rules": "^1.5",
        "jetbrains/phpstorm-attributes": "^1.0"
    },
    "config": {
        "platform-check": true,
        "lock": false
    }
}
